        <dashboard-tile android:icon="@drawable/yl_ic_settings_accounts" android:id="@id/account_settings" android:title="@string/account_settings_title" android:fragment="com.android.settings.accounts.AccountSettings" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_language" android:id="@id/language_settings" android:title="@string/language_settings" android:fragment="com.android.settings.inputmethod.InputMethodAndLanguageSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_gestures" android:id="@id/gestures_settings" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_backup" android:id="@id/privacy_settings" android:title="@string/privacy_settings" android:fragment="com.android.settings.PrivacySettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/header_category_system">
        <dashboard-tile android:icon="@drawable/yl_ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.settings.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_power_on_off" android:id="@id/auto_startup_shutdown_settings" android:title="@string/auto_startup_shutdown_enable">
            <intent android:targetPackage="com.yulong.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.yulong.android.settings.alarm.AutoStartupShutdownSettings" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_timerswitch" android:id="@id/timer_switch_settings" android:title="@string/timer_switch_settings_title" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_accessibility" android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.android.settings.accessibility.AccessibilitySettings" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_print" android:id="@id/print_settings" android:title="@string/print_settings" android:fragment="com.android.settings.print.PrintSettingsFragment" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.settings.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/yl_ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.settings.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>